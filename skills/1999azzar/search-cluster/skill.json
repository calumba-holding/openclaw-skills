{
  "name": "search-cluster",
  "version": "3.5.1",
  "description": "Standard Aggregated Search Engine. Features Google, Wiki, Reddit, GNews, and Scrapling.",
  "author": "Mema",
  "homepage": "https://github.com/1999AZZAR/search-cluster-skill",
  "env_vars": [
    { "name": "GOOGLE_API_KEY", "description": "Google API Key (Optional)", "required": false },
    { "name": "GOOGLE_CSE_ID", "description": "Google CSE ID (Optional)", "required": false },
    { "name": "SCRAPLING_PYTHON_PATH", "description": "Path to scrapling python binary (Optional)", "required": false },
    { "name": "REDIS_HOST", "description": "Redis host for caching (Optional)", "required": false },
    { "name": "REDIS_PORT", "description": "Redis port (Optional, default 6379)", "required": false },
    { "name": "SEARCH_USER_AGENT", "description": "Custom User-Agent (Optional)", "required": false }
  ],
  "binaries": ["python3"],
  "python_dependencies": ["redis", "scrapling"]
}
