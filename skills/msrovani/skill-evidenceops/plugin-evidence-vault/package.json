{
  "name": "@openclaw/evidence-vault",
  "version": "1.0.0",
  "description": "Evidence vault plugin for OpenClaw with chain of custody support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "openclaw",
    "evidence",
    "forensics",
    "chain-of-custody",
    "vault"
  ],
  "author": "OpenClaw Community",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "archiver": "^6.0.0",
    "exifreader": "^4.21.0",
    "filenamify": "^6.0.0",
    "mime-types": "^2.1.35",
    "pino": "^8.18.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/jest": "^29.5.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openclaw/openclaw-evidenceops.git",
    "directory": "plugin-evidence-vault"
  }
}
