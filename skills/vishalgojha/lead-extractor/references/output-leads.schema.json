{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ExtractedLeads",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "lead_id": {
        "type": "string",
        "minLength": 1
      },
      "name": {
        "type": "string",
        "minLength": 1
      },
      "phone": {
        "type": "string",
        "minLength": 1
      },
      "property_type": {
        "type": "string"
      },
      "budget": {
        "type": "number"
      },
      "deal_type": {
        "type": "string",
        "enum": [
          "sale",
          "rent",
          "lease",
          "outright",
          "unknown"
        ]
      },
      "asset_class": {
        "type": "string",
        "enum": [
          "residential",
          "commercial",
          "mixed",
          "pg",
          "unknown"
        ]
      },
      "price_basis": {
        "type": "string",
        "enum": [
          "total",
          "per_sqft",
          "monthly_rent",
          "deposit",
          "unknown"
        ]
      },
      "area_sqft": {
        "type": "number",
        "minimum": 0
      },
      "area_basis": {
        "type": "string",
        "enum": [
          "carpet",
          "rera_carpet",
          "builtup",
          "unknown"
        ]
      },
      "location_hint": {
        "type": "string"
      }
    },
    "required": [
      "lead_id",
      "name",
      "phone"
    ],
    "additionalProperties": false
  }
}
