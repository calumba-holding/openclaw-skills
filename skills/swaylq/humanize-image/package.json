{
  "name": "humanize-image",
  "version": "1.0.0",
  "description": "Humanize AI-generated images to bypass detection. Strip metadata, add film grain, recompress, and make AI images look natural. Works with Midjourney, DALL-E, Stable Diffusion, Flux.",
  "main": "scripts/deai.py",
  "author": "voidborne-d",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voidborne-d/deai-image.git"
  },
  "keywords": [
    "ai-detection",
    "image-processing",
    "metadata-removal",
    "watermark-removal",
    "ai-fingerprint",
    "midjourney",
    "dalle",
    "stable-diffusion",
    "flux",
    "openclaw",
    "agent-skill",
    "image-manipulation"
  ],
  "skills": {
    "name": "deai-image",
    "type": "image-processing",
    "category": "tools",
    "requires": [
      "imagemagick",
      "exiftool"
    ],
    "optional": [
      "python3",
      "pillow",
      "numpy"
    ],
    "commands": {
      "deai": {
        "description": "Remove AI fingerprints from image",
        "exec": "python scripts/deai.py",
        "args": [
          "<input>"
        ],
        "options": {
          "--output": "Output file path",
          "--strength": "Processing strength (light/medium/heavy)",
          "--no-metadata": "Only strip metadata",
          "--batch": "Batch process directory"
        }
      },
      "deai-bash": {
        "description": "Bash version (no Python required)",
        "exec": "bash scripts/deai.sh",
        "args": [
          "<input>",
          "<output>",
          "[strength]"
        ]
      },
      "check-deps": {
        "description": "Check installed dependencies",
        "exec": "bash scripts/check_deps.sh"
      }
    }
  },
  "homepage": "https://github.com/voidborne-d/deai-image#readme",
  "bugs": {
    "url": "https://github.com/voidborne-d/deai-image/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}