{
  "name": "django-claw",
  "version": "1.2.0",
  "description": "Run Django management commands and ORM queries on any Django project.",
  "author": "Manoj",
  "commands": [
    {
      "trigger": "django-claw setup",
      "aliases": ["setup django-claw", "configure django-claw"],
      "script": "setup.sh",
      "args": []
    },
    {
      "trigger": "django-claw models",
      "aliases": ["list models", "show models", "django models"],
      "script": "list-models.sh",
      "args": []
    },
    {
      "trigger": "django-claw apps",
      "aliases": ["list apps", "show apps", "django apps"],
      "script": "list-apps.sh",
      "args": []
    },
    {
      "trigger": "django-claw urls",
      "aliases": ["list urls", "show urls", "django urls", "url patterns"],
      "script": "list-urls.sh",
      "args": []
    },
    {
      "trigger": "django-claw users",
      "aliases": ["list users", "show users", "django users"],
      "script": "list-users.sh",
      "args": []
    },
    {
      "trigger": "django-claw db",
      "aliases": ["db stats", "database stats", "dbstats", "django db stats"],
      "script": "db-stats.sh",
      "args": []
    },
    {
      "trigger": "django-claw pending",
      "aliases": ["pending migrations", "check pending migrations", "any pending migrations"],
      "script": "pending-migrations.sh",
      "args": []
    },
    {
      "trigger": "django-claw settings",
      "aliases": ["settings check", "show settings", "django settings"],
      "script": "settings-check.sh",
      "args": []
    },
    {
      "trigger": "django-claw showmigrations",
      "aliases": ["showmigrations", "show migrations", "migration status"],
      "script": "run.sh",
      "args": ["showmigrations"]
    },
    {
      "trigger": "django-claw makemigrations",
      "aliases": ["makemigrations", "make migrations"],
      "script": "run.sh",
      "args": ["makemigrations"]
    },
    {
      "trigger": "django-claw migrate",
      "aliases": ["migrate", "run migrate", "apply migrations"],
      "script": "run.sh",
      "args": ["migrate"]
    },
    {
      "trigger": "django-claw version",
      "aliases": ["django version", "django --version"],
      "script": "run.sh",
      "args": ["version"]
    },
    {
      "trigger": "django-claw check",
      "aliases": ["django check", "run django check"],
      "script": "run.sh",
      "args": ["check"]
    },
    {
      "trigger": "django-claw python",
      "aliases": ["python version", "which python", "python --version"],
      "script": "python-version.sh",
      "args": []
    },
    {
      "trigger": "django-claw logs",
      "aliases": ["django logs", "show logs", "tail logs"],
      "script": "django-logs.sh",
      "args": []
    },
    {
      "trigger": "django-claw shell",
      "aliases": ["django shell", "run query", "orm query"],
      "script": "run-query.sh",
      "args": ["$input"]
    },
    {
      "trigger": "django-claw readonly",
      "script": "readonly.sh",
      "args": []
    },
    {
      "trigger": "django-claw readonly on",
      "script": "readonly.sh",
      "args": ["on"]
    },
    {
      "trigger": "django-claw readonly off",
      "script": "readonly.sh",
      "args": ["off"]
    }
  ]
}
