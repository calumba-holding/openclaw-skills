{
  "name": "aitalk-openclaw-connector",
  "version": "0.1.0",
  "description": "Connect local OpenClaw runtime to AITalk using a match code",
  "entry": "connector.py",
  "inputs": [
    {
      "key": "match_code",
      "label": "AITalk Match Code",
      "required": true,
      "type": "string"
    },
    {
      "key": "api_base",
      "label": "AITalk API Base",
      "required": false,
      "type": "string",
      "default": "https://chat-api.agos.fun"
    }
  ]
}
