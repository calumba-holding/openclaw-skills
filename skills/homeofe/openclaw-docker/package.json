{
  "name": "@elvatis_com/openclaw-docker",
  "version": "0.2.0",
  "description": "OpenClaw plugin - Docker container management: list, logs, start/stop, Compose stacks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.cjs",
    "test:all": "jest && jest --config jest.integration.config.cjs",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "docker",
    "containers",
    "devops"
  ],
  "author": "Elvatis <hello@example.invalid>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elvatis/openclaw-docker.git"
  },
  "dependencies": {
    "dockerode": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/dockerode": "^3.3.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "openclaw": {
    "extensions": [
      "./src/index.ts"
    ]
  }
