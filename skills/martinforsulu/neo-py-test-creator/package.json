{
  "name": "openclaw-skill-py-test-creator",
  "version": "1.0.0",
  "description": "Automatically generates pytest-compatible unit test templates from Python function signatures and docstrings",
  "main": "scripts/main.py",
  "scripts": {
    "test": "pytest tests/",
    "lint": "flake8 scripts/",
    "generate": "python scripts/cli.py"
  },
  "keywords": [
    "openclaw",
    "skill",
    "testing",
    "pytest",
    "python",
    "unit-tests",
    "ast"
  ],
  "author": "OpenClaw Contributors",
  "license": "MIT",
  "dependencies": {
    "pytest": "^8.0.0",
    "astroid": "^3.0.0"
  },
  "devDependencies": {
    "flake8": "^7.0.0",
    "mypy": "^1.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openclaw/openclaw",
    "directory": "skills/py-test-creator"
  },
  "openclaw": {
    "skillType": "code-generation",
    "capabilities": [
      "python-parsing",
      "test-generation",
      "ast-analysis"
    ],
    "inputs": [
      "python-code",
      "function-signature",
      "docstring"
    ],
    "outputs": [
      "pytest-tests",
      "test-file"
    ]
  }
}
