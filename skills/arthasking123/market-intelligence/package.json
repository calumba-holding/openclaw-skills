        "enabled": false,
        "api_key": "",
        "keywords": []
      },
      "reddit": {
        "enabled": false,
        "subreddits": ["technology", "artificial", "business"]
      }
    },
    "output": {
      "format": "markdown",
      "directory": "./output",
      "include_json": true
    },
    "pricing": {
