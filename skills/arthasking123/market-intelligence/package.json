{
  "name": "market-intelligence",
  "version": "1.0.0",
  "description": "AI Market Intelligence Agent - 自动收集和分析市场/行业信息",
  "main": "main.sh",
  "scripts": {
    "run": "./main.sh",
    "report": "./main.sh --keyword \"\" --format markdown",
    "setup": "./setup.sh"
  },
  "config": {
    "sources": {
      "news": {
        "enabled": false,
        "api_key": "",
        "category": "business,technology"
      },
